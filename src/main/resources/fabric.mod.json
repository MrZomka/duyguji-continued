{
  "schemaVersion": 1,
  "id": "duyguji-client-input",
  "version": "${version}",

  "description": "Input various symbols and emoji in Minecraft via customizable shortcodes.",
  "authors": [
    "Ancurio"
  ],
  "contact": {
    "homepage": "https://github.com/MrZomka/duyguji-continued",
    "sources": "https://github.com/MrZomka/duyguji-continued"
  },

  "license": "LGPLv3",

  "environment": "client",
  "entrypoints": {
    "client": [
      "ancurio.duyguji.client.input.ClientMain"
    ]
  },
  "mixins": [
    "duyguji-client-input.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.8.8",
    "minecraft": "1.18.x"
  },

  "custom": {
    "modupdater": {
      "strategy": "github",
      "owner": "MrZomka",
      "repository": "duyguji-continued"
    }
  }
}
